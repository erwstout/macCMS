{
  "name": "mac-cms",
  "version": "0.0.1",
  "description": "A CMS",
  "main": "index.js",
  "repository": "git@github.com:erwstout/macCMS.git",
  "author": "Eric Stout <ericwstout@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@date-io/moment": "^0.0.2",
    "@material-ui/core": "^3.8.3",
    "@material-ui/icons": "^3.0.1",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "foreman": "^3.0.1",
    "formik": "^1.4.1",
    "html-to-draftjs": "^1.4.0",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "material-ui-pickers": "^2.0.5",
    "moment": "^2.23.0",
    "notistack": "^0.4.1",
    "parcel-bundler": "^1.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.7.1",
    "pug": "^2.0.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-draft-wysiwyg": "^1.12.19",
    "react-router-dom": "^4.3.1",
    "yup": "^0.26.6"
  },
  "scripts": {
    "start": "concurrently \"yarn run login\" \"yarn run admin\"  \"yarn run web\"",
    "web": "nf run -- babel-watch index.js",
    "admin": "parcel watch admin/index.js --out-dir public/admin",
    "login": "parcel watch login/index.js --out-dir public/login",
    "lint": "yarn run eslint .",
    "test": "yarn run lint && yarn run flow",
    "prebuild": "yarn prettier --write ./*.js",
    "clean": " rm -Rf dist/",
    "copy-static-files": "cp package.json dist/package.json",
    "build-structure": "mkdir dist && cd dist && mkdir public",
    "build-server": "parcel build index.js --out-dir dist/ --target node",
    "build-react": "concurrently \"parcel build admin/index.js --out-dir dist/public/admin\" \"parcel build login/index.js --out-dir dist/public/login\"",
    "build": "NODE_ENV=production yarn run prebuild && yarn run clean && yarn run build-structure && yarn run build-server && yarn run build-react",
    "build-preview": "yarn build && nf run -e prod.env -- node dist/index.js"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-watch": "https://github.com/monathur/babel-watch.git",
    "eslint": "^5.12.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-react": "^7.12.3",
    "flow-bin": "^0.89.0",
    "flow-typed": "^2.5.1",
    "prettier": "1.15.3"
  }
}
